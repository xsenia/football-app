{
    "filesGlob": [
        "./*.ts",
        "./**/*.ts",
        "./**/*.tsx"
    ],
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "es6",
        "target": "es6",
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "experimentalDecorators": true,
        "jsx": "react"
    },
    "exclude": [
        "node_modules"
    ]
}
